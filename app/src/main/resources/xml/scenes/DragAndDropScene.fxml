<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="officeolympics.front.controllers.DragAndDropSceneController">
   <Pane fx:id="draggablePane1"
         layoutX="14.0" layoutY="14.0"
         style="-fx-background-color: red;">
      <ImageView fx:id="imageView1"
              fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
   </Pane>
   <Pane fx:id="draggablePane2"
         layoutX="14.0" layoutY="14.0"
         style="-fx-background-color: blue;">
      <ImageView fx:id="imageView2"
                 fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
   </Pane>
   <Pane fx:id="draggablePane3"
         layoutX="14.0" layoutY="14.0"
         style="-fx-background-color: green;">
      <ImageView fx:id="imageView3"
                 fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
   </Pane>
</AnchorPane>
